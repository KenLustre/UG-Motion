{
  "expo": {
    "name": "UGMotion",
    "slug": "UGMotion",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/UGLogo.png",
    "scheme": "ugmotion",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#000000",
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#000000"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/UGLogo.png",
        "backgroundColor": "#000000"
      },
      "backgroundColor": "#000000",
      "package": "com.kenlustre.UGMotion",
      "versionCode": 1
    },
    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./assets/images/UGLogo.png",
          "dark": {
            "image": "./assets/images/UGLogo.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200
        }
      ],
      "expo-router",
      "expo-font"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0a7cf07b-58ac-47c0-910d-d7a30f6dce83"
      }
    }
  }
}
